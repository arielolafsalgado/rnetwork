Package: sen2r
Type: Package
Title: Find, Download and Process Sentinel-2 Data
Version: 1.3.0
Authors@R: c(person("Luigi", "Ranghetti",
                    email = "luigi@ranghetti.info",
                    role = c("aut", "cre"),
                    comment = c(ORCID = "0000-0001-6207-5188")),
             person("Lorenzo", "Busetto",
