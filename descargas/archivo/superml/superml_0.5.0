        ClusterR(>= 1.1), FNN(>= 1.1), ranger(>= 0.10), caret(>= 6.0),
        xgboost(>= 0.6), glmnet(>= 2.0), kableExtra, tm(>= 0.7),
        Matrix(>= 1.2), e1071(>= 1.7)
