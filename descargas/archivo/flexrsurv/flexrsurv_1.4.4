Version: 1.4.4
Date: 2019-12-05
Authors@R: c(
    person("Isabelle", "Clerc-Urmès", role="aut"), 
    person("Michel", "Grzebyk", role = c("aut", "cre"), email = "michel.grzebyk@inrs.fr"),
