Package: pksensi
Type: Package
Title: Global Sensitivity Analysis in Pharmacokinetic Modeling
Version: 1.0.0
Authors@R: c(
    person("Nan-Hung", "Hsieh", role = c("aut", "cre"), email = "nhsieh@cvm.tamu.edu", comment = c(ORCID = "0000-0003-0163-2766")),
