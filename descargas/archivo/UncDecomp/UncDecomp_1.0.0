Package: UncDecomp
Title: Uncertainty Decomposition
Version: 1.0.0
Authors@R: c(person("Seonghyeon", "Kim", email = "shkim93@snu.ac.kr", role = c("aut", "cre")),
	person("Yongdai", "Kim", role = "aut"),
