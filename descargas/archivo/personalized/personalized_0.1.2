Package: personalized
Type: Package
Title: Estimation and Validation Methods for Subgroup Identification
        and Personalized Medicine
Version: 0.1.2
Authors@R: c(
    person("Jared", "Huling",  role = c("aut", "cre"), email = "jaredhuling@gmail.com"),
