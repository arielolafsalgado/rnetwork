Package: antaresProcessing
Type: Package
Title: 'Antares' Results Processing
Version: 0.14
Date: 2018-02-26
Authors@R: c(
    person("Jalal-Edine", "ZAWAM", , "jalal-edine.zawam@rte-france.com", role = c("aut", "cre")),
