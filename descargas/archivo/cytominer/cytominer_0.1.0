Package: cytominer
Type: Package
Title: Methods for Image-Based Cell Profiling
Version: 0.1.0
Authors@R: c(person("Tim", "Becker", email = "tbecker@broadinstitute.org", role = "aut"),
    person("Allen", "Goodman", email = "allen.goodman@icloud.com", role = "aut"),
