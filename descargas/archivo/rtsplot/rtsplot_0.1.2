Package: rtsplot
Type: Package
Title: Time Series Plot
Version: 0.1.2
Authors@R: c(		
	person(family = "RTSVizTeam", role = c("aut", "cph"), email = "rtsvizteam@gmail.com"),
	person("Irina", "Kapler", role = c("cre"), email = "irkapler@gmail.com")
