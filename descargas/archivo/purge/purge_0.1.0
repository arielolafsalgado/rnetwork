Package: purge
Type: Package
Title: Purge Training Data from Models
Version: 0.1.0
Date: 2015-09-01
Authors@R: c(
    person("Marc", "Maier", email="mmaier@massmutual.com", role="cre"),
    person("Chaoqun", "Jia", email="cjia@massmutual.com", role="ctb"),
