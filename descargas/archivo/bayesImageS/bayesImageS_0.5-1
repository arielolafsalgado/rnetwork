Package: bayesImageS
Type: Package
Title: Bayesian Methods for Image Segmentation using a Potts Model
Version: 0.5-1
Date: 2018-01-26
Authors@R: c(
    person("Matt", "Moores", role = c("aut", "cre"), email = "M.T.Moores@warwick.ac.uk", comment = c(ORCID = "0000-0003-4531-3572")),
    person("Kerrie", "Mengersen", role=c("aut", "ths"), email="k.mengersen@qut.edu.au", comment = c(ORCID = "0000-0001-8625-9168")),
