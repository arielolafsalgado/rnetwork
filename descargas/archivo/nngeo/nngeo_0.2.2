Package: nngeo
Type: Package
Title: k-Nearest Neighbor Join for Spatial Data
Version: 0.2.2
Authors@R: c(
    person("Michael", "Dorman", email = "dorman@post.bgu.ac.il", role = c("aut", "cre")),
    person("Johnathan", "Rush", email = "johnathan.rush@mssm.edu", role = c("ctb")),
