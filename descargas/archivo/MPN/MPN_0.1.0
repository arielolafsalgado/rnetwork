Package: MPN
Title: Most Probable Number for Serial Dilutions
Version: 0.1.0
Authors@R: c(
    person("Martine", "Ferguson", email = "Martine.Ferguson@fda.hhs.gov",
	         role = "aut", comment = "original R code"),
