Package: psychrolib
Type: Package
Title: Psychrometric Properties of Moist and Dry Air
Version: 2.4.0
Authors@R: c(
      person("Hongyuan", "Jia", email = "hongyuan.jia@bears-berkeley.sg", role = c("aut", "cre"), comment = c(ORCID = "0000-0002-0075-8183")),
