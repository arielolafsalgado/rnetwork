Package: hmi
Type: Package
Title: Hierarchical Multiple Imputation
Version: 0.8.2
Authors@R: c(
    person("Matthias", "Speidel", , "matthias.speidel@googlemail.com", role = c("aut", "cre"),
    comment = "Institute for Employment Research, Nuremberg, Germany"),
