Package: docxtractr
Title: Extract Data Tables and Comments from 'Microsoft' 'Word'
        Documents
Version: 0.6.1
Authors@R: c(
    person("Bob", "Rudis", email = "bob@rud.is", role = c("aut", "cre"), 
           comment = c(ORCID = "0000-0001-5670-2640")),
    person("Mark", "Dulhunty", role = c("ctb")),
