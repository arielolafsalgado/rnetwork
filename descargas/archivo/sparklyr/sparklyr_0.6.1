Package: sparklyr
Type: Package
Title: R Interface to Apache Spark
Version: 0.6.1
Authors@R: c(
    person("Javier", "Luraschi", email = "javier@rstudio.com", role = c("aut", "cre")),
    person("Kevin", "Ushey", role = "aut", email = "kevin@rstudio.com"),
