Package: politicaldata
Type: Package
Title: Tools for Acquiring and Analyzing Political Data
Version: 0.1.1
Authors@R: 
    c(person(given = "G. Elliott", family = "Morris", role = c("aut","cre"), email = "elliott@thecrosstab.com"),
